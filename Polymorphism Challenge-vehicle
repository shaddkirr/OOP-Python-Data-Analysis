class Vehicle:
    def move(self):
        pass

class Car(Vehicle):
    def move(self):
        print("ğŸš— Driving on the road...")

class Plane(Vehicle):
    def move(self):
        print("âœˆï¸ Flying through the sky...")

class Boat(Vehicle):
    def move(self):
        print("ğŸ›¥ï¸ Sailing on water...")

# Demonstrate polymorphism
vehicles = [Car(), Plane(), Boat()]

for v in vehicles:
    v.move()
